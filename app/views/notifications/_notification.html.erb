<% visitor = notification.visitor %>
<% visited = notification.visited %>
<div class="notification_detail">
  <% case notification.action %>
  <% when "message" %>
  <div class="message">
    <%= link_to rooms_path do %>
      <%= visitor.username %>さんからメッセージが届きました
    <% end %>
    <span class="date"><%= "（" + time_ago_in_words(notification.created_at) + "前）" %></span>
  </div>
  <% end %>
</div>